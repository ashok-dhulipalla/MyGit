<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="nice.school.model.tables.Course" table="course">

		<id name="courseId" type="string" column="course_id">
		</id>
		<property name="courseName">
			<column name="course_name" not-null="true"/>
		</property>

		<property name="createdOn" type="timestamp">
			<column name="created_on" not-null="true"/>
		</property>
		<property name="createdBy">
			<column name="created_by" not-null="true"/>
		</property>
		<property name="lastModifiedOn" type="timestamp">
			<column name="last_modified_on" not-null="true"/>
		</property>

	</class>

</hibernate-mapping>